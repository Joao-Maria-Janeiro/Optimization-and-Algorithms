function task2()
    load('data1.mat');
    x0 = min(X(:, 1)) + (max(X(:, 1))-min(X(:, 1))) .* rand(1,1);
    k = 1;
    epsilon = 10^(-6);
    s = [-1 -1];
    r = [0];
    g = [];
    K = size(Y, 2);
    while (1)
        f = ((s'(k) * exp(s'(k) * x(k) - r(k))) / (exp(s'(k) * x(k) - r(k)) + 1)) - s(k) * Y(k);
        f = log((1 + e^(s'(k) * x(k) - r(k))))
        g(1) = gradient(Y(k))
        k = k + 1;
    end
    g
end